### - - - - - - - - - - - - - - - - - - - - - ###
### GLOBAL ENV VARS
### - - - - - - - - - - - - - - - - - - - - - ###
# NUXT_ENV_RUN_MODE : dev | preprod | prod
NUXT_ENV_RUN_MODE=dev
NUXT_ENV_HOST=localhost
NUXT_ENV_PORT_DEV=5000
NUXT_ENV_PORT_PROD=5001
NUXT_ENV_APP_TITLE="Data patch"

### - - - - - - - - - - - - - - - - - - - - - ###
### LEGAL
### - - - - - - - - - - - - - - - - - - - - - ###
NUXT_ENV_LEGAL_EDITOR_NAME=...
NUXT_ENV_LEGAL_EDITOR_ADRESS=...
NUXT_ENV_LEGAL_EDITOR_TEL=...
NUXT_ENV_LEGAL_EDITOR_CODE=...
NUXT_ENV_LEGAL_PROVIDER_NAME=...
NUXT_ENV_LEGAL_PROVIDER_ADRESS=...
NUXT_ENV_LEGAL_PROVIDER_SITE=...

### - - - - - - - - - - - - - - - - - - - - - ###
### LOCALES / i18n
### - - - - - - - - - - - - - - - - - - - - - ###
NUXT_ENV_LANG_DEFAULT_LOCALE=fr
NUXT_ENV_LANG_DEFAULT_LOCALES=fr:Fran√ßais:fr-FR,en:English:en-US

### - - - - - - - - - - - - - - - - - - - - - ###
### API BACKEND / i18n
### - - - - - - - - - - - - - - - - - - - - - ###
NUXT_API_ROOT_BACKEND_DEV=http://localhost:8000
NUXT_API_ROOT_BACKEND_PROD=https://data-patch.io

NUXT_API_ROOT_PREFIX=/api/v1/
NUXT_API_ROOT_DOCUMENTATION=/api/docs

NUXT_API_USERS=users
